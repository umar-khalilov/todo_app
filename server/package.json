{
    "name": "server",
    "version": "1.0.0",
    "description": "Todo application for everyone",
    "main": "./src/main.js",
    "private": true,
    "scripts": {
        "start": "node ./src/main.js",
        "start:dev": "nodemon ./src/main.js",
        "start:debug": "nodemon --inspect=0.0.0.0:9229 ./src/main.js",
        "lint:fix": "eslint \"src/**/*.js\" --fix",
        "prettier:check": "prettier --check ./",
        "prettier:fix": "prettier --write ./",
        "format": "npm run prettier:fix && npm run lint:fix"
    },
    "type": "commonjs",
    "keywords": [
        "nodejs",
        "express",
        "rest",
        "api",
        "sequelize",
        "postgresql"
    ],
    "author": "Umar Khalilov",
    "license": "ISC",
    "dependencies": {
        "compression": "^1.7.4",
        "cors": "^2.8.5",
        "date-fns": "^2.29.3",
        "dotenv": "^16.0.3",
        "envalid": "^7.3.1",
        "express": "^4.18.2",
        "jsonwebtoken": "^9.0.0",
        "metautil": "^3.7.1",
        "node-schedule": "^2.1.1",
        "nodemailer": "^6.9.1",
        "pg": "^8.10.0",
        "pg-hstore": "^2.3.4",
        "sequelize": "^6.29.3",
        "swagger-ui-express": "^4.6.2",
        "yup": "^1.0.2"
    },
    "devDependencies": {
        "eslint": "^8.36.0",
        "eslint-config-prettier": "^8.7.0",
        "eslint-plugin-prettier": "^4.2.1",
        "nodemon": "^2.0.21",
        "prettier": "^2.8.4",
        "sequelize-cli": "^6.6.0"
    }
}
